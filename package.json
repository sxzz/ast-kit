{
  "name": "ast-kit",
  "version": "1.4.2",
  "packageManager": "pnpm@10.5.2",
  "description": "A toolkit for easy Babel AST generation and manipulation.",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/sxzz/ast-kit#readme",
  "bugs": {
    "url": "https://github.com/sxzz/ast-kit/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/ast-kit.git"
  },
  "author": "三咲智子 Kevin Deng <sxzz@sxzz.moe>",
  "funding": "https://github.com/sponsors/sxzz",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsup",
    "test": "vitest",
    "release": "bumpp && pnpm publish",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@babel/parser": "^7.26.9",
    "pathe": "^2.0.3"
  },
  "devDependencies": {
    "@babel/types": "^7.26.9",
    "@sxzz/eslint-config": "^5.2.0",
    "@sxzz/prettier-config": "^2.2.0",
    "@types/node": "^22.13.9",
    "@vitest/coverage-v8": "^3.0.7",
    "@vitest/ui": "^3.0.7",
    "bumpp": "^10.0.3",
    "eslint": "^9.21.0",
    "estree-walker": "^3.0.3",
    "fast-glob": "^3.3.3",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "5.8.2",
    "vitest": "^3.0.7"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "prettier": "@sxzz/prettier-config"
}
